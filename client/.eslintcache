n      <Route exact path='/' component={Home}/>\n      <Route exact path='/login' component={Login}/>\n      <Route exact path='/register' component={Register}/>\n      <ProtectedRoute exact path='/users' component={Users}/>\n      <ProtectedRoute exact path='/things' component={Things}/>\n      <ProtectedRoute exact path='/user' component={User}/>\n      <Route component={NoMatch} />\n      </Switch>\n    </Container>\n  </FetchUser>\n  </>\n  );\n}\n\nexport default App;\n",["123","124"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/NavBar.js",["125"],"import {Menu} from \"semantic-ui-react\"\nimport {Link, useHistory} from \"react-router-dom\"\nimport styled from \"styled-components\"\nimport { useContext } from \"react\"\nimport { AuthContext } from \"../providers/AuthProvider\"\n\n\nexport default () => {\n\n  const history = useHistory()\n\n  const value = useContext(AuthContext)\n\n  const links = () => {\n    if (value.authenticated){\n      return (\n      <>\n        <Menu.Item onClick={() => value.handleLogout(history)} name=\"Logout\" />\n        <Link to=\"/user\">\n          <MenuText content={value.email} />\n        </Link>\n      </>\n      )\n    }\n    return(\n      <>\n      <Link to=\"/login\">\n        <Menu.Item name='Login'/>\n      </Link>\n      <Link to=\"/register\">\n        <Menu.Item name='Register'/>\n      </Link>\n      \n      </>\n    )\n  }\n\n  return (\n    <Menu>\n      <Link to=\"/\">\n        <Menu.Item name='Home'/>\n      </Link>\n      <Link to=\"/users\">\n        <Menu.Item name='All Users'/>\n      </Link>\n      <Link to=\"/things\">\n        <Menu.Item name='Things'/>\n      </Link>\n      <Menu.Menu position=\"right\">{links()}</Menu.Menu>\n    </Menu>\n  )\n}\n\nconst MenuText = styled(Menu.Item)`\nalign-self: center;\npadding: 5px;\n`\n","/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/demo/Things.js",["126"],"import {Header, Segment, Dimmer, Loader} from \"semantic-ui-react\"\nimport { useEffect, useState } from \"react\"\nimport Axios from \"axios\"\n\nexport default () => {\n\n  const [things, setThings] = useState([])\n  const [loading, setLoading] = useState(true)\n  const [error, setError] = useState(false)\n\n  useEffect(()=>{\n    getThings()\n  },[])\n\n  const getThings = async () => {\n    try { \n      let res = await Axios.get(\"/api/things\")\n      setThings(res.data)\n\n    } catch (err) {\n      console.log(err)\n      setError(true)\n\n    } finally {\n      setLoading(false)\n    }\n  }\n\n  const renderThings = ()=>{\n    if (loading){ return(\n      <Segment>\n      <Dimmer active>\n        <Loader />\n      </Dimmer>\n    </Segment>\n    )\n   }\n    if (error){ return <p>Please login in to see this page.</p> }\n    return things.map((t, index)=>{\n      return (\n        <div key={index} >\n          {t.name}\n        </div>\n      )\n    })\n  }\n\n\n  return (\n    <>\n    <Header>Things</Header>\n    {renderThings()}\n    </>\n  )\n}","/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/providers/AuthProvider.js",["127"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/Login.js",["128"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/Register.js",["129"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/Users.js",[],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/index.js",[],["130","131"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/NoMatch.js",["132"],"import {Header} from \"semantic-ui-react\"\n\nexport default () => {\n  return (\n    <Header>Page does not exist</Header>\n  )\n\n}","/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/demo/Home.js",["133"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/FetchUser.js",["134"],"import { useContext, useState, useEffect } from \"react\"\nimport { AuthContext } from \"../providers/AuthProvider\"\nimport Axios from \"axios\"\n\nconst FetchUser = (props) => {\n\n  const [token, setToken] = useState(false)\n\n  const {authenticated, setUser} = useContext(AuthContext)\n\n  useEffect(()=>{\n    getUser()\n  },[])\n\n  const getUser = async () => {\n    if (authenticated || !localStorage.getItem('access-token')) {\n      setToken(true)\n      return; \n    }\n    \n    try {\n      const res = await Axios.get(\"/api/auth/validate_token\")\n      console.log(res.data.data)\n      setUser(res.data.data)\n    } catch (err) {\n      console.log(err)\n    } finally {\n      setToken(true)\n    }\n  }\n\n  return token ? props.children : null\n}\n\nexport default FetchUser","/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/ProtectedRoute.js",[],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week7/react-rails-starter-with-auth/client/src/components/User.js",[],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/providers/AuthProvider.js",["135"],"import React from \"react\"\nimport { useState } from \"react\"\nimport Axios from \"axios\"\nimport { useHistory } from \"react-router-dom\"\n\n\nexport const AuthContext = React.createContext()\n\nconst AuthProvider = (props) => {\n\n  const handleRegister = async (user, history) => {\n    try {\n      let res = await Axios.post(\"/api/auth\", user)\n      console.log(\"user: \", res.data.data)\n      setUser(res.data.data)\n      history.push(\"/user\")\n    } catch (err) {\n      alert(\"Error occurred while attempting to register user. Please Debug for more information\")\n    }\n  }\n\n  const handleLogin = async (user, history) => {\n    try {\n      let res = await Axios.post(\"/api/auth/sign_in\", user)\n      setUser(res.data.data)\n      history.push(\"/user\")\n      console.log(res.data.data.email)\n    } catch (err) {\n      alert(\"Error occurred while attempting to Login user. Please Debug for more information\")\n    }\n  }\n\n  const handleLogout = async (history) => {\n    // debugger\n    try {\n      await Axios.delete(\"/api/auth/sign_out\")\n      setUser(null)\n      history.push(\"/login\")\n\n    } catch (err) {\n      alert(\"Error occurred while attempting to Logout user. Please Debug for more information\")\n    }\n    \n  }\n\n\n  const [user, setUser] = useState(null)\n\n  const auth = {\n    ...user,\n    handleRegister,\n    handleLogin,\n    handleLogout,\n    authenticated: user !== null,\n    setUser,\n  }\n\n  return(\n    <AuthContext.Provider value = {auth}>\n      {props.children}\n    </AuthContext.Provider>\n  )\n}\n\nexport default AuthProvider",["136","137"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/components/ProtectedRoute.js",[],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/components/User.js",[],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/components/NavBar.js",["138"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/demo/Home.js",["139"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/components/DropZoneDemo.js",["140"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/components/Login.js",["141"],"/Users/landonwhitesides/Desktop/DPL_WINTER_2020/week10/image-upload/client/src/components/NoMatch.js",["142"],{"ruleId":"143","severity":1,"message":"144","line":3,"column":1,"nodeType":"145","endLine":8,"endColumn":2},{"ruleId":null,"fatal":true,"severity":2,"message":"146","line":13,"column":3},{"ruleId":"143","severity":1,"message":"144","